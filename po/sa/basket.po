#: devtools/weaver/main.cpp:18
#, kde-format
msgid "basketweaver"
msgstr "टोकरीच्छेदकः"

#: devtools/weaver/main.cpp:19
#, kde-format
msgid "Encodes and decodes .baskets files"
msgstr ".baskets सञ्चिकाः एन्कोड् करोति, डिकोड् च करोति"

#: devtools/weaver/main.cpp:28
#, kde-format
msgid "Destination directory. Optional."
msgstr "गन्तव्यनिर्देशिका । वैकल्पिक।"

#: devtools/weaver/main.cpp:33
#, kde-format
msgid ""
"Use the <image> as preview image for a .baskets file. Must be a .png file."
msgstr ""
".baskets सञ्चिकायाः पूर्वावलोकनप्रतिबिम्बरूपेण <image> इत्यस्य उपयोगं कुर्वन्तु । .png "
"सञ्चिका भवितुमर्हति ।"

#: devtools/weaver/main.cpp:38
#, kde-format
msgid "Decodes the <file>. Cannot be used together with --weave."
msgstr "<file> इत्यस्य डिकोडं करोति । --weave इत्यनेन सह एकत्र उपयोक्तुं न शक्यते।"

#: devtools/weaver/main.cpp:43
#, kde-format
msgid "Encodes the <directory>. Cannot be used together with --unweave."
msgstr "<निर्देशिका> एन्कोड् करोति । --unweave इत्यनेन सह एकत्र उपयोक्तुं न शक्यते ।"

#: devtools/weaver/main.cpp:48
#, kde-format
msgid ""
"This optional value will be used to label the new content within the "
"optionally given output directory"
msgstr ""
"वैकल्पिकरूपेण दत्तस्य आउटपुट् निर्देशिकायाः अन्तः नूतनसामग्री लेबलं कर्तुं एतत् वैकल्पिकं मूल्यं "
"उपयुज्यते"

#: devtools/weaver/main.cpp:52
#, kde-format
msgid "Overwrite existing files."
msgstr "विद्यमानसञ्चिकाः अधिलिखन्तु ।"

#: devtools/weaver/weaver.cpp:44
#, kde-format
msgid "You need to provide at least one --weave/-w or --unweave/-u option"
msgstr "भवद्भिः न्यूनातिन्यूनम् एकं --weave/-w अथवा --unweave/-u विकल्पं दातव्यम्"

#: devtools/weaver/weaver.cpp:49
#, kde-format
msgid "You cannot use --weave/-w and --unweave/-u options in conjunction."
msgstr "--weave/-w तथा --unweave/-u विकल्पयोः संयोजनेन उपयोगं कर्तुं न शक्नुवन्ति ।"

#: devtools/weaver/weaver.cpp:55
#, kde-format
msgid ""
"Multiple --weave/-w or --unweave/-u input options found. All but the first "
"input are going to be ignored."
msgstr ""
"अनेकाः --weave/-w अथवा --unweave/-u निवेशविकल्पाः प्राप्ताः । प्रथमं निवेशं विहाय सर्वे "
"उपेक्षितुं गच्छन्ति।"

#: devtools/weaver/weaver.cpp:79
#, kde-format
msgid "The source seems to be an invalid .baskets file"
msgstr "स्रोतः अमान्यः .baskets सञ्चिका इव दृश्यते"

